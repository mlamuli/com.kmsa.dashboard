<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<ViewSettingsDialog
		confirm="handleFilterDialogConfirm">
		<filterItems>
			<ViewSettingsFilterItem
				text="Status"
				key="Status"
				multiSelect="false">
				<items>
					<ViewSettingsItem text="Open" key="A" />
					<ViewSettingsItem text="Packed" key="B" />
					<ViewSettingsItem text="Checked" key="C" />
				</items>
			</ViewSettingsFilterItem>
		</filterItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>
